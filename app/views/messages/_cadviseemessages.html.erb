



<h2>Completed Advisee Messages</h2>

<table class="table table-striped">
  <tr>

    <th></th>
    <th>User</th>
    <th>Advisor</th>
    <th>Advicepost</th>
    <th>Category Name</th>
	<th>Advisor Name</th>
    <th>Created On</th>


  </tr>

<% @messages.each do |message| %>
  <tr>
	<td><%= link_to 'View', message %></td>
    <td><%= message.user_id %></td>
    <td><%= message.advisor_id %></td>
    <td><%= message.advicepost_id %></td>
    <td><%= message.advicepost.categoryname %></td>
    <td><%= message.advicepost.user.name %></td>
	<td><%= message.created_at %></td>


  </tr>
<% end %>
</table>

<br />





